        for i in range(1, logk):        
            if(((k>>i)%2)==1):
                qc.compose(I_CDKM_carryless(m+1-i), qubits = mul[m+i:2*m] + ancilla[n+2+2*m-nn:n+3+2*m-nn] + mul[:m+1-i] + ancilla[0:1], inplace=True)                    
