        for i in range(n-3):
            qc.x(a[i+2])
            qc.cx(b[i+1],a[i+2])

        qc.cx(b[n-2], a[n-1])

        for i in range(n-3):
            qc.ccx(b[n-i-3], a[n-i-2], b[n-i-2])
            qc.x(a[n-i-2])
            qc.cx(b[n-i-1], b[n-i-2])

        qc.ccx(ancilla[0], a[1], b[1])
        qc.x(a[1])
        qc.cx(b[2], b[1])

        qc.ccx(a[0], b[0], ancilla[0])
        qc.cx(b[1], ancilla[0])
        
        for i in range(n):
            qc.cx(b[i], a[i])
