        qc.ccx(a[n-2-i], b[n-2-i], b[n-1-i])
    
    qc.ccx(controlled[0], b[0], a[0])
    
    for i in range(n-2):
        qc.cx(b[i+1], b[i+2])
    
    for i in range(n-1):
        qc.cx(b[i+1], a[i+1])
    
